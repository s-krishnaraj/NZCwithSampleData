    "FORMULA0" : {
      "action" : "formula",
      "sources" : [ "JOIN2" ],
      "parameters" : {
        "expressionType" : "SQL",
        "fields" : [ {
          "type" : "TEXT",
          "name" : "EU_Object",
          "label" : "EU Object",
          "formulaExpression" : "case \n    when startsWith(Id, '7LK') then 'Stationary'\n    when startsWith(Id, '6Wo') then 'Vehicles'\n    when startsWith(Id, '0o1') then 'Air Travel'\n    when startsWith(Id, '0or') then 'Freight Hauling'\n    when startsWith(Id, '0pE') then 'Ground Travel'\n    when startsWith(Id, '0pK') then 'Hotel Stay'\n    when startsWith(Id, '0pX') then 'Rental Car'\n    when startsWith(Id, '0pd') then 'Procurement'\nend",
          "precision" : 255,
          "defaultValue" : ""
        } ]
      }
    },
    "FORMULA2" : {
      "action" : "formula",
      "sources" : [ "EDIT_ATTRIBUTES0" ],
      "parameters" : {
        "expressionType" : "SQL",
        "fields" : [ {
          "type" : "TEXT",
          "name" : "EU_ParentGroup",
          "label" : "Energy Use Parent Group",
          "formulaExpression" : "case when \"EmissionSource.AssetConfig.StationaryAssetType\" in ('CommercialBuilding', 'DataCenter') then \"EURecordType.Name\"\nwhen \"EmissionSource.AssetConfig.StationaryAssetType\" in ('FleetVehicle', 'PrivateJet') then 'Vehicles'\nwhen \"EU_Object\" in ('Air Travel', 'Ground Travel', 'Hotel Stay', 'Rental Car') then 'Travel' \nwhen \"EU_Object\" = 'Freight Hauling' then 'Freight' \nwhen \"EU_Object\" = 'Procurement' then 'Procurement'\nelse null end\n\n",
          "precision" : 255,
          "defaultValue" : ""
        } ]
      }
    },
    "FORMULA3" : {
      "action" : "formula",
      "sources" : [ "EDIT_ATTRIBUTES1" ],
      "parameters" : {
        "expressionType" : "SQL",
        "fields" : [ {
          "type" : "TEXT",
          "name" : "EU_RecordType",
          "label" : "Energy Use Record Type",
          "formulaExpression" : "case when \"EU_Object\" in ('Stationary', 'Vehicles') then \"EURecordType.Name\" else \"EU_Object\" end",
          "precision" : 255,
          "defaultValue" : ""
        } ]
      }
    },
    "FORMULA4" : {
      "action" : "formula",
      "sources" : [ "FORMULA7" ],
      "parameters" : {
        "expressionType" : "SQL",
        "fields" : [ {
          "type" : "NUMBER",
          "name" : "Distance_KM",
          "label" : "Distance (KM)",
          "formulaExpression" : "case \n    when \"DistanceUnit\" = 'Miles' then \"Distance\" * 1.60934\n    when \"DistanceUnit\" = 'Nautical Miles' then \"Distance\" * 1.852\n else \"Distance\" end",
          "precision" : 10,
          "scale" : 2,
          "defaultValue" : ""
        } ]
      }
    },
    "FORMULA5" : {
      "action" : "formula",
      "sources" : [ "FORMULA4" ],
      "parameters" : {
        "expressionType" : "SQL",
        "fields" : [ {
          "type" : "NUMBER",
          "name" : "Distance_Miles",
          "label" : "Distance (Miles)",
          "formulaExpression" : "case \n    when \"DistanceUnit\" = 'Kilometers' then \"Distance\" * 0.621371\n    when \"DistanceUnit\" = 'Nautical Miles' then \"Distance\" * 1.15078\n else \"Distance\" end",
          "precision" : 10,
          "scale" : 2,
          "defaultValue" : ""
        } ]
      }
    },
    "FORMULA6" : {
      "action" : "formula",
      "sources" : [ "FORMULA5" ],
      "parameters" : {
        "expressionType" : "SQL",
        "fields" : [ {
          "type" : "NUMBER",
          "name" : "Scope3EmissionsInTco2e_Derived",
          "label" : "Scope3 Emissions (Tco2e) - Derived",
          "formulaExpression" : "coalesce(\"Scope3EmissionsInTco2e\", 0) +\ncoalesce(\"Scope3UpstrmEmissionsInTco2e\", 0) +\ncoalesce(\"Scope3DnstrmEmissionsInTco2e\", 0)",
          "precision" : 14,
          "scale" : 2,
          "defaultValue" : ""
        } ]
      }
    },
    "FORMULA7" : {
      "action" : "formula",
      "sources" : [ "FORMULA8" ],
      "parameters" : {
        "expressionType" : "SQL",
        "fields" : [ {
          "type" : "TEXT",
          "name" : "IsBusinessTravel",
          "label" : "Is Business Travel",
          "formulaExpression" : "case when \"EU_ParentGroup\" = 'Travel' OR \"EURecordType.MasterLabel\" = 'PrivateJet' then 'True' else 'False' end",
          "precision" : 255,
          "defaultValue" : ""
        } ]
      }
    },
    "FORMULA8" : {
      "action" : "formula",
      "sources" : [ "FORMULA3" ],
      "parameters" : {
        "expressionType" : "SQL",
        "fields" : [ {
          "type" : "TEXT",
          "name" : "IsElectricity",
          "label" : "IsElectricity",
          "formulaExpression" : "case when FuelType = 'Electricity' then 'True' else 'False' end",
          "precision" : 255,
          "defaultValue" : ""
        } ]
      }
    },
    "FORMULA9" : {
      "action" : "formula",
      "sources" : [ "FORMULA6" ],
      "parameters" : {
        "expressionType" : "SQL",
        "fields" : [ {
          "type" : "NUMBER",
          "name" : "Total_Emissions_w_LB",
          "label" : "Total Emissions with LB",
          "formulaExpression" : "coalesce(\"Scope1EmissionsInTco2e\", 0) + coalesce(\"Scope2LocBasedEmssnInTco2e\", 0) + coalesce(\"Scope3EmissionsInTco2e_Derived\", 0)",
          "precision" : 10,
          "scale" : 2,
          "defaultValue" : ""
        } ]
      }
    },
    "FORMULA10" : {
